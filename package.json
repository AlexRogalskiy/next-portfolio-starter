{
  "name": "next-portfolio-starter",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "dev": "npx netlify-cms-proxy-server & next dev & node admin/config-watcher.js",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "predeploy:dryrun": "yarn build && yarn export",
    "predeploy:prod": "yarn build && yarn export",
    "deploy:dryrun": "netlify deploy",
    "deploy:prod": "netlify deploy --prod",
    "lint": "eslint pages/ functions/",
    "lint-fix": "eslint pages/ functions/ --fix"
  },
  "lint-staged": {
    "./**/*.{js,json,css}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "*.js": "eslint --cache --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.2.4",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chokidar": "^3.5.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "netlify-cms-app": "^2.14.23",
    "next": "10.0.7",
    "pino": "^6.11.1",
    "postcss": "^8.2.6",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "tailwindcss": "^2.0.3"
  }
}
