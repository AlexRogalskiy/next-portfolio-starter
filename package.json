{
  "name": "next-portfolio-starter",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "scripts": {
    "dev": "npx netlify-cms-proxy-server & next dev & node admin/config-watcher.js",
    "prebuild": "rm -rf out && cp admin/config.yml public/config.yml",
    "build": "NODE_ENV=production next build",
    "postbuild": "node transforms/img-optimisation.js && next export && node transforms/html-minifier.js",
    "start": "next start",
    "lint": "eslint ./",
    "lint-fix": "eslint ./ --fix",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "pages/**/*.js": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "*.js": "eslint --cache --fix"
  },
  "dependencies": {
    "@storybook/addon-postcss": "^2.0.0",
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.2.4",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chokidar": "^3.5.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "globby": "^11.0.2",
    "gray-matter": "^4.0.2",
    "html-minifier": "^4.0.0",
    "jsdom": "^16.4.0",
    "netlify-cms-app": "^2.14.23",
    "next": "10.0.7",
    "next-mdx-remote": "^2.1.3",
    "next-remote-watch": "^0.3.0",
    "pino": "^6.11.1",
    "postcss": "^8.2.6",
    "prettier": "^2.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sharp": "^0.27.1",
    "tailwindcss": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@storybook/addon-actions": "^6.2.0-alpha.32",
    "@storybook/addon-essentials": "^6.2.0-alpha.32",
    "@storybook/addon-links": "^6.2.0-alpha.32",
    "@storybook/react": "^6.2.0-alpha.32",
    "babel-loader": "^8.2.2"
  }
}
